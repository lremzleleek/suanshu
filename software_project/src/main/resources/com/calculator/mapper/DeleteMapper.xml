<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.calculator.mapper.DeleteMapper">
    <delete id="delete">
        delete from fiveadd where id in
        <foreach collection="deleteId" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </delete>



</mapper>