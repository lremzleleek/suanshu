.horn{
		  background-color: yellow;
        position: absolute;
        width: 100%;
        height: 100%;
        border:3px solid #eec866;
        margin: 100px;
}
.horn>div{
        width: 10px;
        height: 10px;
        position:absolute;
}
.horn .lt{
		 
        border-top: 6px solid #eec866;
        border-left: 6px solid #eec866;
        left: -6px;
        top: -6px;
}
.horn .rt{
        border-top: 6px solid #eec866;
        border-right: 6px solid #eec866;
        right: -6px;
        top: -6px;
}
.horn .rb{
        border-bottom:6px solid #eec866;
        border-right: 6px solid #eec866;
        right: -6px;
        bottom: -6px;
}
.horn .lb{
        border-bottom:6px solid #eec866;
        border-left: 6px solid #eec866;
        left: -6px;
        bottom: -6px;
}
