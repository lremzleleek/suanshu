<view class="all"><view class="bar"><view class="previous" bindtap="{{a}}"><button class="barbtn">上一月</button></view><view class="date">{{b}} 年 {{c}} 月</view><view class="next" bindtap="{{d}}"><button class="barbtn">下一月</button></view></view><view class="week"><view wx:for="{{e}}" wx:for-item="item" wx:key="b" class="week_ri">{{item.a}}</view></view><view class="myDateTable"><view wx:for="{{f}}" wx:for-item="item" wx:key="h" class="dateCell"><view wx:if="{{item.a}}" class="cell"><text decode="{{true}}">  </text></view><view wx:else bindtap="{{item.g}}"><view wx:if="{{item.b}}" class="cell yellow" style="background:#0000e7;color:#ffffff"><text>{{item.c}}</text></view><view wx:elif="{{item.d}}" class="cell white bg-red" style="background:#ffaa7f;color:#ffffff"><text>{{item.e}}</text></view><view wx:else class="whiteColor cell"><text>{{item.f}}</text></view></view></view></view></view>